#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules/astro/node_modules:/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules:/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules/astro/node_modules:/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules:/mnt/c/Users/Hunte/Documents/GitHub/thatsealgair.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@4.7.1_@types+node@18.19.33_typescript@5.4.5/node_modules/astro/astro.js" "$@"
fi
